#
# Godot Game project EditorConfig
#
# @version 1.0.0
#

root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 2
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false

[*.gd]
indent_style = tab
indent_size = 4
insert_final_newline = false
trim_trailing_whitespace = false

[*.cs]
indent_size = 4
